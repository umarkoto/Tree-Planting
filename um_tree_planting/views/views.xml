        <!-- Membuat View Search Tree Planting-->
        <record id="um_treeplanting_view_search" model="ir.ui.view">
            <field name="name">um.treeplanting.search</field>
            <field name="model">um.treeplanting</field>
            <field name="arch" type="xml">
                <search string="Search">
                    <searchpanel>
                        <field name="entitas" icon="fa-signal" enable_counters="1"/>
                    </searchpanel>
                </search>
            </field>
        </record>

        <record id="um_treeplanting_view_tree" model="ir.ui.view">
            <field name="name">um.treeplanting.tree</field>
            <field name="model">um.treeplanting</field>
            <field name="arch" type="xml">
                <tree string="Tree Planting List">
                    <field name="name"/>
                    <field name="nama_penanam"/>
                    <field name="entitas"/>
<!--                    <field name="tree_line"/>-->
                </tree>
            </field>
        </record>

        <record id="training_session_view_graph" model="ir.ui.view">
            <field name="name">um.treeplanting.graph</field>
            <field name="model">um.treeplanting</field>
            <field name="arch" type="xml">
                <graph string="Session" sample="1" stacked="False" type="pie" order="desc">
                    <field name="entitas"/>
                    <!--                    <field name="tree_line"/>-->
                </graph>
            </field>
        </record>

        <!--        aksi tanam pohon-->
        <record id="tree_planting_action" model="ir.actions.act_window">
            <field name="name">Tree Planting</field>
            <field name="res_model">um.treeplanting</field>
            <field name="view_mode">tree,graph,form,kanban,pivot</field>
            <field name="help" type="html">
                <p name="o_view_nocontent_smiling_face">
                    Input Data Pertama Anda ...
                </p>
            </field>
        </record>

        <record id="update_satu" model="ir.actions.act_window">
            <field name="name">Update Pohon</field>
            <field name="res_model">update.pohon</field>
            <!--            <field name="context">{'search_default_account_id': [active_id]}</field>-->
        </record>
    </data>
